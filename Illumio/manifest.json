{
    "name": "Illumio",
    "id": "Illumio",
    "version": "1.0.0",
    "description": "This plugin pulls labels from Illumio PCE",
    "patch_supported": true,
    "push_supported": true,
    "configuration": [
        {
            "label": "PCE URL",
            "key": "api_url",
            "type": "text",
            "default": "",
            "mandatory": true,
            "description": "Illumio API Base URL."
        },
        {
            "label": "API Username",
            "key": "auth_username",
            "type": "text",
            "default": "",
            "mandatory": true,
            "description": "Illumio API Username"
        },
        {
            "label": "API PAssword",
            "key": "auth_password",
            "type": "password",
            "default": "",
            "mandatory": true,
            "description": "Illumio API password"
        },
        {
            "label": "Initial Range (in days)",
            "key": "days",
            "type": "number",
            "mandatory": false,
            "default": 7,
            "description": "Number of days to pull the data for the initial run."
        }
    ]
}

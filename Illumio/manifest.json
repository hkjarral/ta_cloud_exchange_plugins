{
    "name": "Illumio",
    "id": "Illumio",
    "version": "1.0.0",
    "description": "This plugin pulls labels from Illumio PCE",
    "patch_supported": true,
    "push_supported": true,
    "configuration": [
        {
            "label": "PCE url",
            "key": "api_url",
            "type": "text",
            "default": "https://login.illum.io",
            "mandatory": true,
            "description": "Illumio API Base URL."
        },
        {
            "label": "Port",
            "key": "api_port",
            "type": "number",
            "default": 443,
            "mandatory": true,
            "description": "Illumio API Port Number."
        },
        {
            "label": "API Authentication Username",
            "key": "api_username",
            "type": "text",
            "default": "",
            "mandatory": true,
            "description": "Illumio API Username"
        },
        {
            "label": "API Secret",
            "key": "api_password",
            "type": "password",
            "default": "",
            "mandatory": true,
            "description": "Illumio API password"
        },
        {
            "label": "Organization ID",
            "key": "org_id",
            "type": "number",
            "mandatory": true,
            "default": "1",
            "description": "Organization ID"
        },
        {
            "label": "Label ID",
            "key": "label_id",
            "type": "text",
            "mandatory": true,
            "default": "quarantine",
            "description": "Label ID"
        }
    ]
}

{
    "name": "Illumio",
    "id": "Illumio",
    "version": "1.0.0",
    "description": "The illumio plugin pulls labels from Illumio PCE",
    "patch_supported": false,
    "push_supported": false,
    "configuration": [
        {
            "label": "Username",
            "key": "key_username",
            "type": "text",
            "default": "",
            "mandatory": true,
            "description": "Authentication Username"
        },
        {
            "label": "Key Secret",
            "key": "key_secret",
            "type": "password",
            "default": "",
            "mandatory": true,
            "description": "Authentication Secret"
        },
        {
            "label": "Label ID",
            "key": "label_id",
            "type": "test",
            "default": "Quarantine",
            "mandatory": true,
            "description": "Label ID"
        },
        {
            "label": "Enable Tagging",
            "key": "enable_tagging",
            "type": "choice",
            "choices": [
                {
                    "key": "Yes",
                    "value": "Yes"
                },
                {
                    "key": "No",
                    "value": "No"
                }
            ],
            "default": "Yes",
            "mandatory": true,
            "description": "Enable/Disable tagging functionality."
        }
    ]
}